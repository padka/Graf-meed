дивидуальный дизайн мебели от Графика-Мебель\" />\n        <img src={img2} alt=\"Изготовление мебели на заказ с учетом всех пожеланий клиента\" />\n        <img src={img3} alt=\"Примеры готовой мебели от компании Графика-Мебель\" />\n      </div>\n    </section>\n  );\n}\n\nexport default IntroSection;\n"],"mappings":";AAAA,OAAOA,KAAK,MAAM,OAAO;AACzB,OAAO,oBAAoB;AAC3B,OAAO,iBAAiB;AACxB,OAAOC,IAAI,MAAM,mBAAmB;AACpC,OAAOC,IAAI,MAAM,mBAAmB;AACpC,OAAOC,IAAI,MAAM,mBAAmB;AAAC,SAAAC,MAAA,IAAAC,OAAA;AAErC,SAASC,YAAYA,CAAA,EAAG;EACtB,oBACED,OAAA;IAASE,SAAS,EAAC,eAAe;IAAAC,QAAA,gBAChCH,OAAA;MAAAG,QAAA,EAAI;IAAgB;MAAAC,QAAA,EAAAC,YAAA;MAAAC,UAAA;MAAAC,YAAA;IAAA,OAAI,CAAC,eACzBP,OAAA;MAAAG,QAAA,EAAG;IAEH;MAAAC,QAAA,EAAAC,YAAA;MAAAC,UAAA;MAAAC,YAAA;IAAA,OAAG,CAAC,eACJP,OAAA;MAAAG,QAAA,EAAG;IAEH;MAAAC,QAAA,EAAAC,YAAA;MAAAC,UAAA;MAAAC,YAAA;IAAA,OAAG,CAAC,eACJP,OAAA;MAAKE,SAAS,EAAC,SAAS;MAAAC,QAAA,gBACtBH,OAAA;QAAKQ,GAAG,EAAEZ,IAAK;QAACa,GAAG,EAAC;MAAgD;QAAAL,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA,OAAE,CAAC,eACvEP,OAAA;QAAKQ,GAAG,EAAEX,IAAK;QAACY,GAAG,EAAC;MAA8D;QAAAL,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA,OAAE,CAAC,eACrFP,OAAA;QAAKQ,GAAG,EAAEV,IAAK;QAACW,GAAG,EAAC;MAAmD;QAAAL,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA,OAAE,CAAC;IAAA;MAAAH,QAAA,EAAAC,YAAA;MAAAC,UAAA;MAAAC,YAAA;IAAA,OACvE,CAAC;EAAA;IAAAH,QAAA,EAAAC,YAAA;IAAAC,UAAA;IAAAC,YAAA;EAAA,OACC,CAAC;AAEd;AAACG,EAAA,GAjBQT,YAAY;AAmBrB,eAAeA,YAAY;AAAC,IAAAS,EAAA;AAAAC,YAAA,CAAAD,EAAA"},"metadata":{},"sourceType":"module","externalDependencies":[]}